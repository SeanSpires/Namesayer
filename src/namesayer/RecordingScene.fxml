<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #98fb98;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="namesayer.RecordingController">
   <children>
      <Button fx:id="confirmRecord" layoutX="268.0" layoutY="200.0" mnemonicParsing="false" onMouseClicked="#clickConfirmRecord" text="Record" />
      <Button layoutX="512.0" layoutY="340.0" mnemonicParsing="false" onMouseClicked="#clickRecordQuit" text="Cancel" />
      <Text fx:id="recordText" fill="GHOSTWHITE" layoutX="144.0" layoutY="141.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Press the record button to start recording" textAlignment="CENTER" wrappingWidth="0.0">
         <font>
            <Font size="15.0" />
         </font></Text>
      <ProgressBar fx:id="progressBar" layoutX="206.0" layoutY="263.0" prefWidth="200.0" progress="0.0" />
   </children>
   <cursor>
      <Cursor fx:constant="HAND" />
   </cursor>
   <effect>
      <Lighting diffuseConstant="1.36">
         <bumpInput>
            <Shadow />
         </bumpInput>
         <light>
            <Light.Distant />
         </light>
      </Lighting>
   </effect>
</AnchorPane>
